#!/bin/sh

bash /home/philipp/.fehbg &
bash /home/philipp/bin/lightsOn.sh &
(while true; do bash /home/philipp/bin/autorandr.sh; done) &
(while true; do conky -c ~/.conkyrc; done) &
(while true; do tp-battery-icon; done) &
cairo-dock -o &
