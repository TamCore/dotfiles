defaults.pcm.rate_converter "speexrate_medium"

pcm.!default {
  type plug
  slave {
    pcm softvol
  }
}

pcm.softvol {
  type softvol
  slave {
    pcm dsp
  }
  control {
    name SoftMaster
    card Intel
  }
}

pcm.dsp {
  type plug
  slave {
    pcm dmixer
  }
}

pcm.dmixer {
  type dmix
  ipc_key 1024
  ipc_key_add_uid 0
  ipc_perm 0660
  bindings {
    0 0
    1 1
  }
  slave {
    pcm hdmi_out
  }
}

pcm.hdmi_out {
  type hw
  card Intel
  device 3
}

pcm.laptop_out {
  type hw
  card Intel
  device 0
}

